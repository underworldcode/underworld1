<param name="Parent">Stg_Component</param>
<param name="Reference">...</param>
<param name="Summary">...</param>
<param name="Description">The parent class for how Shapes (geometries) are defined in Underworld. This class provides data structures and polymoric functions for its children classes - as such there should never be an instance of this class, only its children.

                   +ve y
                    ^
                    |
                    |          -ve z
                    |        /
                    |      /
                    |    /
                    |  /
      -ve x _______________________________ +ve x
                  / |
                /   |
              /     |
            /       |
          +ve z     -ve y

</param>

<!--Now the interesting stuff-->


<list name="Params">
	<struct>
		<param name="Name">CentreX</param>
		<param name="Type">Double</param>
		<param name="Default">0.0</param>
		<param name="Description">x coord the shape is centred on</param>
	</struct>
	<struct>
		<param name="Name">CentreY</param>
		<param name="Type">Double</param>
		<param name="Default">0.0</param>
		<param name="Description">y coord the shape is centred on</param>
	</struct>
	<struct>
		<param name="Name">CentreZ</param>
		<param name="Type">Double</param>
		<param name="Default">0.0</param>
		<param name="Description">z coord the shape is centred on</param>
	</struct>
	<struct>
		<param name="Name">alpha</param>
		<param name="Type">Double</param>
		<param name="Default">0.0</param>
		<param name="Description">Anticlockwise rotation, in degrees, around the z axis as viewed from +ve z</param>
	</struct>
	<struct>
		<param name="Name">beta</param>
		<param name="Type">Double</param>
		<param name="Default">0.0</param>
		<param name="Description">Anticlockwise rotation, in degrees, around the y axis as viewed from +ve y</param>
	</struct>
	<struct>
		<param name="Name">gamma</param>
		<param name="Type">Double</param>
		<param name="Default">0.0</param>
		<param name="Description">Anticlockwise rotation, in degrees, around the x axis as viewed from +ve x</param>
	</struct>

</list>

<list name="Dependencies">

</list>
<!-- Add an exmaple XML if possible -->
<param name="Example">N/A</param>

</StGermainData>
