<!-- Rayleigh-Taylor lucifer viewports -->
<!-- DTD to validate against -->
<StGermainData xmlns="http://www.vpac.org/StGermain/XML_IO_Handler/Jun2003">

   <struct name="components" mergeType="merge">

      <struct name="TwoColour">
            <param name="Type">lucColourMap</param>
            <param name="colours">Green Red</param>
            <param name="minimum">0.0</param>
            <param name="maximum">1.0</param>
            <param name="dynamicRange">true</param>
      </struct>
      <struct name="TriColour1">
            <param name="Type">lucColourMap</param>
            <param name="colours">Blue:0.1 Green:0.3 Red:0.7</param>
            <param name="minimum">0.0</param>
            <param name="maximum">1.0</param>
            <param name="dynamicRange">true</param>
      </struct>
      <struct name="TriColour2">
            <param name="Type">lucColourMap</param>
            <param name="colours">Red Yellow Black</param>
            <param name="minimum">-1.0</param>
            <param name="maximum">1.0</param>
            <param name="dynamicRange">false</param>
      </struct>   
      <struct name="Rainbow">
            <param name="Type">lucColourMap</param>
            <param name="colours">Purple Blue Green Yellow Orange Red</param>
            <param name="minimum">0.0</param>
            <param name="maximum">1.0</param>
            <param name="logScale">f</param>
            <param name="dynamicRange">t</param>
      </struct>   
      <struct name="Rainbow2">
            <param name="Type">lucColourMap</param>
            <param name="colours">Black Purple Blue Green Yellow Orange Red</param>
            <param name="minimum">0.0</param>
            <param name="maximum">1.0</param>
            <param name="centreOnFixedValue">true</param>
            <param name="centreValue">0.7</param>
            <param name="logScale">f</param>
            <param name="dynamicRange">t</param>
      </struct>   
      <struct name="Rainbow3">
            <param name="Type">lucColourMap</param>
            <param name="colours">Purple Blue Green Yellow Orange Red</param>
            <param name="minimum">1.0</param>
            <param name="maximum">20.0</param>
            <param name="logScale">t</param>
            <param name="dynamicRange">f</param>
      </struct>

      <struct name="ColourVal1a">
            <param name="Type">lucColourMap</param>
            <param name="colours">Green (5)Yellow Orange Red</param>
            <param name="minimum">1.0</param>
            <param name="maximum">11.0</param>
      </struct>
      <struct name="ColourVal1b">
            <param name="Type">lucColourMap</param>
            <param name="colours">(1)Green (5)Yellow Orange (11)Red</param>
      </struct>

      <struct name="ColourVal2a">
            <param name="Type">lucColourMap</param>
            <param name="minimum">-0.5</param>
            <param name="maximum">1.5</param>
            <param name="colours">Green (0.5)Yellow Orange Red</param>
            <param name="dynamicRange">t</param>
      </struct>
      <struct name="ColourVal2b">
            <param name="Type">lucColourMap</param>
            <param name="colours">(-0.5)Green (0.5)Yellow Orange (1.5)Red</param>
            <param name="dynamicRange">t</param>
      </struct>

      <struct name="ColourValLog1a">
            <param name="Type">lucColourMap</param>
            <param name="colours">Green (5)Yellow Orange Red</param>
            <param name="minimum">1.0</param>
            <param name="maximum">12.0</param>
            <param name="logScale">t</param>
      </struct>
      <struct name="ColourValLog1b">
            <param name="Type">lucColourMap</param>
            <param name="colours">(1)Green (5)Yellow Orange (12)Red</param>
            <param name="logScale">t</param>
      </struct>

      <struct name="ColourValLog2a">
            <param name="Type">lucColourMap</param>
            <param name="colours">Green (0.5)Yellow Orange Red</param>
            <param name="minimum">0.01</param>
            <param name="maximum">1.0</param>
            <param name="logScale">1</param>
            <param name="dynamicRange">t</param>
      </struct>
      <struct name="ColourValLog2b">
            <param name="Type">lucColourMap</param>
            <param name="colours">(0.1)Green (0.5)Yellow Orange (1.1)Red</param>
            <param name="logScale">1</param>
            <param name="dynamicRange">t</param>
      </struct>

      <struct name="TwoColourBar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">TwoColour</param>
         <param name="margin">100</param>
         <param name="height">22</param>
         <param name="borderWidth">2</param>
         <param name="precision">3</param>
         <param name="scientific">True</param>
         <param name="ticks">4</param>
         <param name="scaleValue">2</param>
      </struct>
      <struct name="TriColour1Bar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">TriColour1</param>
         <param name="margin">150</param>
         <param name="ticks">5</param>
         <param name="printTickValue">False</param>
      </struct>
      <struct name="TriColour2Bar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">TriColour2</param>
         <param name="ticks">10</param>
         <param name="tick1">0.5</param>
         <param name="tick2">0.0</param>
         <param name="tick3">-0.25</param>
         <param name="tick4">-0.5</param>
         <param name="tick5">-0.6</param>
         <param name="tick6">-0.7</param>
         <param name="tick7">-0.8</param>
         <param name="tick8">-0.89</param>
         <param name="tick9">-0.95</param>
         <param name="tick10">-0.99</param>
         <param name="lengthFactor">0.95</param>
         <param name="margin">200</param>
      </struct>
      <struct name="RainbowBar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">Rainbow</param>
         <param name="margin">250</param>
         <param name="ticks">10</param>
      </struct>
      <struct name="Rainbow2Bar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">Rainbow2</param>
         <param name="margin">300</param>
      </struct>
      <struct name="Rainbow3Bar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">Rainbow3</param>
         <param name="margin">350</param>
      </struct>
      <struct name="ColourVal1aBar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">ColourVal1a</param>
         <param name="ticks">1</param>
         <param name="tick1">5.0</param>
         <param name="margin">400</param>
      </struct>
      <struct name="ColourVal1bBar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">ColourVal1b</param>
         <param name="ticks">1</param>
         <param name="tick1">5.0</param>
         <param name="margin">450</param>
      </struct>
      <struct name="ColourVal2aBar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">ColourVal2a</param>
         <param name="ticks">1</param>
         <param name="tick1">0.5</param>
         <param name="margin">500</param>
      </struct>
      <struct name="ColourVal2bBar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">ColourVal2b</param>
         <param name="ticks">1</param>
         <param name="tick1">0.5</param>
         <param name="margin">550</param>
      </struct>
      <struct name="ColourValLog1aBar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">ColourValLog1a</param>
         <param name="ticks">3</param>
         <param name="tick1">5.0</param>
         <param name="margin">600</param>
      </struct>
      <struct name="ColourValLog1bBar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">ColourValLog1b</param>
         <param name="margin">650</param>
      </struct>
      <struct name="ColourValLog2aBar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">ColourValLog2a</param>
         <param name="ticks">9</param>
         <param name="margin">700</param>
      </struct>
      <struct name="ColourValLog2bBar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">ColourValLog2b</param>
         <param name="margin">750</param>
      </struct>

      <struct name="strainRateInvColourMap2">
         <param name="Type">lucColourMap</param>
         <param name="colours">DarkRed FireBrick Red OrangeRed Yellow</param>
         <param name="dynamicRange">True</param>
      </struct>
      <struct name="strainRateInvColourBar">
         <param name="Type">lucColourBar</param>
         <param name="ColourMap">strainRateInvColourMap2</param>
         <param name="ticks">3</param>
         <param name="tick1">.001</param>
         <param name="tick2">.002</param>
         <param name="printUnits">1</param>
         <param name="borderWidth">0</param>
      </struct>
      <struct name="strainRateInvMap2">
         <param name="Type">lucScalarField</param>
         <param name="FieldVariable">StrainRateInvariantField</param>
         <param name="ColourMap">strainRateInvColourMap2</param>
         <param name="resolution">2</param>
         <param name="opacity">0.01</param>
      </struct>

      <!-- Viewport -->
      <struct name="lucColourBarTestVP">
         <param name="Type">lucViewport</param>
         <param name="Camera">camera</param>
         <list name="DrawingObject">
            <param>strainRateInvColourBar</param>
            <param>strainRateInvMap2</param>
            <param>TwoColourBar</param>
            <param>TriColour1Bar</param>
            <param>TriColour2Bar</param>
            <param>RainbowBar</param>
            <param>Rainbow2Bar</param>
            <param>Rainbow3Bar</param>
            <param>ColourVal1aBar</param>
            <param>ColourVal1bBar</param>
            <param>ColourVal2aBar</param>
            <param>ColourVal2bBar</param>
            <param>ColourValLog1aBar</param>
            <param>ColourValLog1bBar</param>
            <param>ColourValLog2aBar</param>
            <param>ColourValLog2bBar</param>
         </list>
      </struct>
		<struct name="windowColourBar">
			<param name="Type">lucDefaultWindow</param>
			<param name="Database">database</param>
			<param name="height">800</param>
			<param name="width">800</param>
			<param name="antialias">false</param>
   		<list name="Viewport">
		   	<param>lucColourBarTestVP</param>
   		</list>
		</struct>
   </struct>

</StGermainData>
