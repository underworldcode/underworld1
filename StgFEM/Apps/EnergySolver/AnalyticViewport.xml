<?xml version="1.0"?>
<!DOCTYPE StGermainData SYSTEM "stgermain.dtd">

<StGermainData xmlns="http://www.vpac.org/StGermain/XML_IO_Handler/Jun2003">

   <param name="WindowWidth">800</param>

   <list name="ViewportLayout">
      <param>Temperature Analytic</param>
   </list>

   <list name="Viewports" mergeType="merge">
      <struct>
         <param name="Name">Analytic</param>
         <param name="CameraName">camera</param>

         <list name="Objects">
            <struct>
               <param name="FeVariable">AnalyticTemperatureField</param>
               <param name="PlotAs">FeVariableSurface</param>
               <param name="ColourMap">CIItcomTemperatureColourMap</param>
               <param name="Wireframe">False</param>
            </struct>   
            <struct>
               <param name="FeVariable">AnalyticTemperatureField</param>
               <param name="PlotAs">FeVariableSurface</param>
               <param name="ColourMap">Black</param>
