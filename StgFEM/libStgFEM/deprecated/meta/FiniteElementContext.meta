<?xml version="1.0"?>
<!DOCTYPE StGermainData SYSTEM "stgermain.dtd">
<StGermainData xmlns="http://www.vpac.org/StGermain/XML_IO_Handler/Jun2003">

<param name="Name">FiniteElementContext</param>
<param name="Organisation">VPAC</param>
<param name="Project">StgFEM</param>
<param name="Location">./StgFEM/SLE/SystemSetup/src/</param>
<param name="Project Web">http://www.stgermainproject.org/StgFEM.html</param>
<param name="Copyright">Copyright (C) 2004-2005 VPAC.</param>
<param name="License">The Gnu Lesser General Public License v2.1 - http://www.gnu.org/licenses/old-licenses/lgpl-2.1.html</param>
<param name="Parent">DomainContext</param>
<param name="Description">Builds on the DomainContext to allow a FEM problem to be constructed and solved</param>

<list name="Params">
	<struct>
		<param name="Name">limitTimeStepIncreaseRate</param>
		<param name="Type">Bool</param>
		<param name="Default">False</param>
		<param name="Description">Determines whether the increase in Dt should be limited</param>
	</struct>
	<struct>
		<param name="Name">maxTimeStepIncreasePercentage</param>
		<param name="Type">Double</param>
		<param name="Default">10.0</param>
		<param name="Description">Sets a limit on the percentage increase in Dt per timestep (requires limitTimeStepIncreaseRate to be True)</param>
	</struct>
   <struct>
		<param name="Name">saveCoordsWithFields</param>
		<param name="Type">Bool</param>
		<param name="Default">False</param>
		<param name="Description">Determines whether coordinates are output with field values in checkpoint files</param>
	</struct>
</list>

<list name="Dependencies">
</list>

<param name="Example">n/a</param>

</StGermainData>
