<?xml version="1.0"?>
<!DOCTYPE StGermainData SYSTEM "stgermain.dtd">
<!-- A StGermain input file -->
<!-- DTD to validate against -->
<StGermainData xmlns="http://www.vpac.org/StGermain/XML_IO_Handler/Jun2003">

	<struct name="components" mergeType="merge">
		<struct name="linearMesh">
			<param name="Type">FeMesh</param>
			<param name="elementType">linear</param>
			<param name="UseFeAlgorithms">False</param>
		</struct>

		<struct name="linearMeshGenerator">
			<param name="Type">CartesianGenerator</param>
			<param name="mesh">linearMesh</param>
			<param name="dims">dim</param>
			<param name="shadowDepth">shadowDepth</param>
			<param name="regular">False</param>
			<list name="size">
				<param>elementResI</param>
				<param>elementResJ</param>
				<param>elementResK</param>
			</list>
			<list name="minCoord">
				<param>minX</param>
				<param>minY</param>
				<param>minZ</param>
			</list>
			<list name="maxCoord">
				<param>maxX</param>
				<param>maxY</param>
				<param>maxZ</param>
			</list>
			<list name="enabledDims">
				<param>0</param>
				<param>1</param>
				<param>2</param>
				<param>3</param>
			</list>
			<list name="enabledIncidence" mergeType="merge">
				<param>0</param><param>0</param>
				<param>1</param><param>0</param>
				<param>2</param><param>0</param>
				<param>3</param><param>0</param>
				<param>3</param><param>1</param>
				<param>3</param><param>2</param>
				<param>0</param><param>1</param>
				<param>0</param><param>2</param>
				<param>0</param><param>3</param>
				<param>1</param><param>3</param>
				<param>2</param><param>3</param>
			</list>
		</struct>
	</struct>

</StGermainData>
