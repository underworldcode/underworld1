<?xml version="1.0"?>
<!DOCTYPE StGermainData SYSTEM "stgermain.dtd">

<StGermainData xmlns="http://www.vpac.org/StGermain/XML_IO_Handler/Jun2003">

	<struct name="components" mergeType="merge">
		<struct name="surfaceTemperature">
			<param name="Type">lucScalarFieldOnMeshCrossSection</param>
			<param name="FieldVariable">TemperatureField</param>
			<param name="ColourMap">temperatureColourMap</param>
			<param name="resolution">16</param>
			<param name="crossSection">y=max</param>
			<param name="cullFace">False</param> 
		</struct>

		<struct name="Viewport" mergeType="merge">
			<list name="DrawingObject" mergeType="merge">
				<param>surfaceTemperature</param>
			</list>
		</struct>
	</struct>
</StGermainData>
